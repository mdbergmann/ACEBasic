;BEGIN Picasso96
Assign Picasso96: "Sys:System/Picasso96"
;END Picasso96

;BEGIN MUI
if exists "Workbench:System/MUI"
   assign MUI: "Workbench:System/MUI"
   if exists MUI:Libs
      assign add LIBS: MUI:Libs
   endif
   if exists MUI:Locale
      assign add LOCALE: MUI:Locale
   endif
   version >nil: exec.library 39
   if not warn
      if exists MUI:Docs
         if exists HELP:dummy ; do not remove
         endif                ; this entry!
         assign add HELP: MUI:Docs
      endif
   endif
endif
;END MUI

;BEGIN UnixDirs
run >nil: C:UnixDirsII
;END UnixDirs

;BEGIN ADE
if exists SYS:Tools/ADE
   assign ADE: SYS:Tools/ADE
   execute ADE:ADE-Startup dir=ADE:
   assign include: gg:include
endif
assign AmiTcp: sys:
;END ADE

